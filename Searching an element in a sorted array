
        int s=0, l=N-1;
        
        
  while(s<=l){
            
        int mid = s + (l - s) / 2;
         
          if(arr[mid]==K){
              return 1;
          }
          else if(arr[mid]<K){
              s=mid+1;
          }
          else{
             l=mid-1;
          }
          
          
          
         
      }
      return -1;
        
