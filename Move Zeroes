class Solution {
    void pushZerosToEnd(int[] nums, int n) {
        // code here
       int j=0;
       for(int i=0;i<n;i++){
           if(nums[i]==0){
             
           }
           else{
               int temp=nums[i];
               nums[i]=nums[j];
               nums[j]=temp;
                j++;
            
           }
       }
        
        
